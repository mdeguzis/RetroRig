	<item><!-- Customizar Menu -->
	<label fallback="31207">$INFO[skin.string(settingscustom.label)]</label>
	<icon fallback="-">$INFO[skin.string(settingscustom.bg)]</icon>
	<property name="condition">$INFO[skin.string(settingscustom.submenu),on)]</property>
	<property name="empty">true</property>
	<property name="type">default</property>
	<property name="name">settingscustom</property>
	<onclick condition="!Skin.HasSetting(custommode)">Close</onclick>
	<onclick condition="!Skin.HasSetting(custommode)">ActivateWindow(6000)</onclick>
	<onclick condition="!Skin.HasSetting(custommode)">Skin.SetBool(custommode)</onclick>
	<onclick condition="Skin.HasSetting(custommode)">SetFocus(5120)</onclick>
	<visible>[SubString(skin.string(settingscustom.submenu),on) | Skin.HasSetting(custommode)]</visible>
